{
  "name": "smart-irrigation",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Kory Porter <kory.porter@hotmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:dev": "yarn concurrently -n client,server \"yarn workspace @smart-irrigation/client run start \" \"yarn workspace @smart-irrigation/server run start:dev\"",
    "start:micro-controller": "yarn concurrently -n server,watcher \"http-server -p 8080\" \"yarn ts-node watch.ts\"",
    "deploy:server": "caprover deploy -c captain-definition-server -a smart-irrigation-server"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/http-server": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.1.0",
    "nodemcu-tool": "^3.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  },
  "dependenciesMeta": {
    "@prisma/client@3.9.2": {
      "unplugged": true
    },
    "prisma@3.9.2": {
      "unplugged": true
    }
  }
}
